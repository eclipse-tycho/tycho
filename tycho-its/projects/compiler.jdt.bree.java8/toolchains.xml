<?xml version="1.0" encoding="UTF8"?>
<toolchains>
  <toolchain>
    <type>jdk</type>
    <provides>
      <version>1.8</version>
      <id>JavaSE-1.8</id>
    </provides>
    <configuration>
      <jdkHome>/usr/lib/jvm/java-8-openjdk-amd64</jdkHome>
      <bootClassPath>
        <includes>
          <include>jre/lib/*</include>
          <include>jre/lib/ext/*</include>
          <include>jre/lib/endorsed/*</include>
        </includes>
      </bootClassPath>
    </configuration>
  </toolchain>
</toolchains>
