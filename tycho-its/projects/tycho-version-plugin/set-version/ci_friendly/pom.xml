	<modelVersion>4.0.0</modelVersion>
	<artifactId>test.artifact</artifactId>
	<groupId>org.tycho.its</groupId>
	<packaging>eclipse-plugin</packaging>
	<properties>
		<revision>1.0.0-SNAPSHOT</revision>
