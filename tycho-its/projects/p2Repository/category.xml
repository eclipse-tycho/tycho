<?xml version="1.0" encoding="UTF-8"?>
<!-- includes three bundles in different ways; note: the bundles do not require each other -->
<site>
   <bundle id="osgi.enterprise" version="0.0.0"/>
   <iu id="org.apache.felix.gogo.runtime" version="0.0.0">
     <category name="Test Category" />
   </iu>
   <iu>
     <query><expression type="match">id == "javax.annotation"</expression></query>
     <category name="Test Category" />
   </iu>
   <category-def label="Test Category Label" name="Test Category">
     <description>Test Category Description</description>
   </category-def>
</site>
